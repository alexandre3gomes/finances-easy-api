<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-reverse-engineering PUBLIC "-//Hibernate/Hibernate Reverse Engineering DTD 3.0//EN" "http://hibernate.org/dtd/hibernate-reverse-engineering-3.0.dtd" >

<hibernate-reverse-engineering>
	<type-mapping>
		<sql-type jdbc-type="INTEGER" hibernate-type="int"
			not-null="true">
		</sql-type>
		<sql-type jdbc-type="VARCHAR" hibernate-type="string"
			length="50" not-null="true">
		</sql-type>
		<sql-type jdbc-type="TIMESTAMP" hibernate-type="timestamp"
			length="35" not-null="false">
		</sql-type>
		<sql-type jdbc-type="NUMERIC" hibernate-type="big_decimal"
			precision="131089" not-null="true">
		</sql-type>
	</type-mapping>

	<table-filter match-schema="public" match-name="budget" />
	<table-filter match-schema="public" match-name="budget_categories" />
	<table-filter match-schema="public" match-name="category" />
	<table-filter match-schema="public" match-name="expense" />
	<table-filter match-schema="public" match-name="income" />
	<table schema="public" name="budget">
		<column name="id"></column>
		<column name="end_date"></column>
		<column name="app_user"></column>
		<column name="start_date"></column>
	</table>
	<table schema="public" name="budget_categories">
		<column name="category_id"></column>
		<column name="budget_id"></column>
		<column name="value"></column>
	</table>
	<table schema="public" name="category">
		<column name="id"></column>
		<column name="name"></column>
	</table>
	<table schema="public" name="expense">
		<column name="id"></column>
		<column name="name"></column>
		<column name="expire_at"></column>
		<column name="app_user"></column>
		<column name="category"></column>
		<column name="value"></column>
	</table>
	<table schema="public" name="income">
		<column name="id"></column>
		<column name="value"></column>
		<column name="name"></column>
		<column name="app_user"></column>
		<column name="date"></column>
	</table>
</hibernate-reverse-engineering>